package ba.bitcamp.hajrudin.programming.example;

public class Main {

	public static void main(String[] args) {
		try{
		Card c1 = new Card(10, 0);
		Card c2 = new Card(12, 3);
		Card c3 = new Card(13, 2);
		Card c4 = new Card(1, 1);
		}catch(Exception ex){
			System.out.println("Wrong input");
		}
		System.out.println(c1.getCard());
		System.out.println(c2.getCard());
		System.out.println(c3.getCard());
		System.out.println(c4.getCard());
	}

}
